import java.util.Comparator;

public class CustomerPriorityComparator implements Comparator {

	@Override
	public int compare(Customer st) {
		if (history.getCreditRating() < st.history.getCreditRating()) {
			return 1;
		} else if (history.getCreditRating() == st.history.getCreditRating()) {
			return 0;
		} else {
			return -1;
		}

		
	}

